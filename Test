coroutine.wrap(PQVIGUR_fake_script)()
local function TOIPCQ_fake_script() -- MainFrame.base 
	local script = Instance.new('LocalScript', MainFrame)

	UserInputService = game:GetService("UserInputService")
	local UI = game.Parent
	
	
	local function changeuistate(input, _gameProcessed)
		if input.UserInputType == Enum.UserInputType.K then
			if UI.Visible == true then
				UI.Visible = false
			else
				UI.Visible = true
			end
		end
	end
	
	UserInputService.InputBegan:Connect(changeuistate)
end
coroutine.wrap(TOIPCQ_fake_script)()
local function YZVKRL_fake_script() -- MainFrame.T close Open 
	local script = Instance.new('LocalScript', MainFrame)

	local UserInputService = game:GetService("UserInputService")
	
	function onKeyPress(inputObject, gameProcessedEvent)
		if not gameProcessedEvent then
	    if inputObject.KeyCode == Enum.KeyCode.T then 
				if script.Parent.Parent.MainFrame.Visible == false then
					script.Parent.Parent.MainFrame.Visible = true
				else
					script.Parent.Parent.MainFrame.Visible = false
				end
			end
		end
	end
	
	UserInputService.InputBegan:connect(onKeyPress)
end
coroutine.wrap(YZVKRL_fake_script)()
